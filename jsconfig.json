{
    "compilerOptions": {
        "target": "ES2023",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        
        /* Type checking */
        "jsx": "react-jsx",
        "checkJs": true,
        
        /* Alias */
        "baseUrl": "src",
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": ["src"]
}